from random import randint

# Generates a number from 1 through 10 inclusive
random_number = randint(1, 10)

guesses_left = 3
while guesses_left>0:
  guess=input('What is your guess?')
  if guess==random_number:
    print('You win!')
    guesses_left-=1
    break
else:
    print('You lose.')

# Start your game!
